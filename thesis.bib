@article{Arif2010,
abstract = {Researchers have developed many models to predict and understand human$\backslash$nperformance in text entry. Most of the models are specific to a$\backslash$ntechnology or fail to account for human factors and variations in system$\backslash$nparameters, and the relationship between them. Moreover, the process of$\backslash$nfixing errors and its effects on text entry performance has not been$\backslash$nstudied. Here, we first analyze real-life text entry error correction$\backslash$nbehaviors. We then use our findings to develop a new model to predict$\backslash$nthe cost of error correction for character-based text entry$\backslash$ntechnologies. We validate our model against quantities derived from the$\backslash$nliterature, as well as with a user study. Our study shows that the$\backslash$npredicted and observed costs of error correction correspond well. At the$\backslash$nend, we discuss potential applications of our new model.},
