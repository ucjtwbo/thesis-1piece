issn = {0278-7393},
journal = {Journal of experimental psychology. Learning, memory, and cognition},
keywords = {Adolescent,Adult,Analysis of Variance,Attention,Attention: physiology,Choice Behavior,Choice Behavior: physiology,Cognition,Cognition: physiology,Female,Humans,Judgment,Judgment: physiology,Male,Memory,Memory: physiology,Perception,Perception: physiology,Professional Competence,Psychomotor Performance,Psychomotor Performance: physiology,Reaction Time,Reaction Time: physiology,Vocabulary,Young Adult},
number = {5},
