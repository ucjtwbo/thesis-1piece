@article{Barchard2011,
abstract = {Human data entry can result in errors that ruin statistical results and conclusions. A single data entry error can make a moderate correlation turn to zero and a significant t-test non-significant. Therefore, researchers should design and use human computer interactions that minimize data entry errors. In this paper, 195 undergraduates were randomly assigned to three data entry methods: double entry, visual checking, and single entry. After training in their assigned method, participants entered 30 data sheets, each containing six types of data. Visual checking resulted in 2958\% more errors than double entry, and was not significantly better than single entry. These data entry errors sometimes had terrible effects on coefficient alphas, correlations, and t-tests. For example, 66\% of the visual checking participants produced incorrect values for coefficient alpha, which was sometimes wrong by more than.40. Moreover, these data entry errors would be hard to detect: Only 0.06\% of the errors were blank or outside of the allowable range for the variables. Thus, researchers cannot rely upon histograms and frequency tables to detect data entry errors. Single entry and visual checking should be replaced with more effective data entry methods, such as double entry. ?? 2011 Elsevier Ltd. All rights reserved.},
author = {Barchard, Kimberly A. and Pace, Larry A.},
file = {:Volumes/jborghou/ToRead/Barchard2011.pdf:pdf},
isbn = {0747-5632},
issn = {07475632},
journal = {Computers in Human Behavior},
keywords = {Data cleaning,Data entry,Double entry,Outliers,Visual checking},
number = {5},
pages = {1834--1839},
publisher = {Elsevier Ltd},
title = {{Preventing human error: The impact of data entry methods on data accuracy and statistical results}},
volume = {27},
year = {2011}
}
@article{Barchard2013,
abstract = {Data entry errors can have catastrophic effects on the results of a statistical analysis. Therefore, researchers often spend considerable effort checking their data. This paper compared the effectiveness of three data checking methods - double entry, read aloud, and visual checking - using the types of data and data entry personnel that are typically used in psychological research. To compare these techniques, we created 20 data sheets and entered them into the computer. Next, we deliberately introduced errors into this data set. Participants' job was to locate and correct these errors. A total of 340 undergraduates participated in this study. Of these, 80 had previous data entry experience and 260 did not. Double entry was far superior to read aloud and visual checking, both among people with previous data entry experience and among people without previous experience. Among people with no previous experience, read aloud and visual checking had more than 20 times as many errors as double entry. In addition, double entry was preferred over visual checking. Thus, although double entry takes slightly longer, it is clearly worth the extra effort. ?? 2013 Elsevier Ltd.All rights reserved .},
author = {Barchard, Kimberly A. and Verenikina, Yevgeniya},
doi = {10.1016/j.chb.2013.02.021},
file = {:Volumes/jborghou/ToRead/Barchard2013.pdf:pdf},
issn = {07475632},
journal = {Computers in Human Behavior},
keywords = {Data checking,Double entry,Read aloud,Visual checking},
number = {5},
pages = {1917--1922},
publisher = {Elsevier Ltd},
title = {{Improving data accuracy: Selecting the best data checking technique}},
volume = {29},
year = {2013}
}
